{
    "compilerOptions": {
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "target": "es5",
      "allowJs": true,
      "baseUrl": ".",
      "outDir": "out",
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules", "./dist/", "./out/**/*"]
  }
  