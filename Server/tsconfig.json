{
    "compilerOptions": {
      "sourceMap": true,
      "strict": true,
      "noImplicitReturns": true,
      "noImplicitAny": false,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "target": "es5",
      "allowJs": true,
      "baseUrl": ".",
      "outDir": "out",
    },
    // "include": ["./index.d.ts", "**/*.ts"],
    "exclude": ["node_modules", "./dist/", "./out/**/*"]
  }
  